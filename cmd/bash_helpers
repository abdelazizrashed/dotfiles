## You need to source this file in ~/.bash_profile for these commands to work
## Example:
## source ~/.config/cmd/bash_helpers


alias test="echo \"bash working\""
alias python=python3
alias pip=pip3
alias py=python3


alias ff=.fvm/flutter_sdk/bin/flutter
alias workdir="cd /Users/aziz/Documents/work/projects/"


alias gendata="py ~/.config/cmd/fluttergen/main.py"

# export GOBIN=$HOME/go/bin
